{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-console": "off",
    "prettier/prettier": ["error", { "trailingComma": "es5" }],
    "react/prop-types": 0
  }
}
